---
chdir: <%= @dir %>
environment: <%= @environment %>
address: 0.0.0.0
port: <%= @port %>
ssl: true
ssl_key_file: /etc/ssl/transit-demo.admin.umass.edu.key
ssl_cert_file: /etc/ssl/transit-demo.admin.umass.edu.cert
timeout: 30
log: log/thin.log
pid: tmp/pids/thin.pid
max_conns: 1024
max_persistent_conns: 512
require: []
wait: 30
daemonize: true
rackup: private_pub.ru
